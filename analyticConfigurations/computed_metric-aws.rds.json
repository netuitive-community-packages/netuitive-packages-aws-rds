{
  "analyticConfiguration": {
    "metrics": [
      {
        "match": "netuitive.aws.rds.totalthroughput",
        "properties": {
          "expressions": [
            "${aws.rds.readthroughput}.actual + ${aws.rds.writethroughput}.actual"
          ],
          "fqn": "netuitive.aws.rds.totalthroughput",
          "name": "Total Throughput"
        }
      },
      {
        "match": "netuitive.aws.rds.totaliops",
        "properties": {
          "expressions": [
            "${aws.rds.readiops}.actual + ${aws.rds.writeiops}.actual"
          ],
          "fqn": "netuitive.aws.rds.totaliops",
          "name": "Total IOPS"
        }
      },
      {
        "match": "netuitive.aws.rds.averagereadsize",
        "properties": {
          "expressions": [
            "${aws.rds.readthroughput}.actual / ${aws.rds.readiops}.actual",
            "0"
          ],
          "fqn": "netuitive.aws.rds.averagereadsize",
          "name": "Average Read Size"
        }
      },
      {
        "match": "netuitive.aws.rds.averagewritesize",
        "properties": {
          "expressions": [
            "${aws.rds.writethroughput}.actual / ${aws.rds.writeiops}.actual",
            "0"
          ],
          "fqn": "netuitive.aws.rds.averagewritesize",
          "name": "Average Write Size"
        }
      },
      {
        "match": "netuitive.aws.rds.connectionthroughpututilizationpercent",
        "properties": {
          "expressions": [
            "100 * (${aws.rds.readlatency}.actual * ${aws.rds.readiops}.actual + ${aws.rds.writelatency}.actual * ${aws.rds.writeiops}.actual) / ${aws.rds.databaseconnections}.actual",
            "0"
          ],
          "fqn": "netuitive.aws.rds.connectionthroughpututilizationpercent",
          "name": "Connection Throughput Utilization Percent"
        }
      },
      {
        "match": "netuitive.aws.rds.diskspacepercentused",
        "properties": {
          "expressions": [
            "100 * (attribute(allocatedStorage) - (${aws.rds.freestoragespace}.actual/1024/1024/1024)) / attribute(allocatedStorage)"
          ],
          "fqn": "netuitive.aws.rds.diskspacepercentused",
          "name": "Disk Space Percent Used"
        }
      },
      {
        "match": "netuitive.aws.rds.iopsutilization",
        "properties": {
          "expressions": [
            "Math.min(100, ${netuitive.aws.rds.totaliops}.actual * 100 / (!attribute(iops) ? (attribute(allocatedStorage) * 3 < 100 ? 100 : (attribute(allocatedStorage) * 3 > 10000 ? 10000 : attribute(allocatedStorage) * 3)) : attribute(iops)))"
          ],
          "fqn": "netuitive.aws.rds.iopsutilization",
          "name": "IOPS Utilization"
        }
      }
    ],
    "name": "AWS RDS",
    "scope": {
      "elementType": "RDS"
    },
    "type": "COMPUTED_METRIC"
  }
}